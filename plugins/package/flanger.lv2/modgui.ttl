@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/creesullivan/cephean/flanger>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-cephean-flanger.html> ;
        modgui:stylesheet <modgui/stylesheet-cephean-flanger.css> ;
        modgui:screenshot <modgui/screenshot-cephean-flanger.png> ;
        modgui:thumbnail <modgui/thumbnail-cephean-flanger.png> ;
        modgui:brand "cephean" ;
        modgui:label "FLANGER" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "darkblue" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "depth" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "stop1" ;
            lv2:name "Stop 1" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "stop2" ;
            lv2:name "Stop 2" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "rate" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "tone" ;
            lv2:name "Tone" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "level" ;
            lv2:name "Level" ;
        ] ;
    ] .