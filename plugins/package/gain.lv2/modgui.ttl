@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://lv2plug.in/plugins/eg-gain>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gain-control.html> ;
        modgui:stylesheet <modgui/stylesheet-gain-control.css> ;
        modgui:screenshot <modgui/screenshot-gain-control.png> ;
        modgui:thumbnail <modgui/thumbnail-gain-control.png> ;
        modgui:brand "cephean" ;
        modgui:label "gain control" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "petrol" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "level" ;
            lv2:name "Gain" ;
        ] ;
    ] .