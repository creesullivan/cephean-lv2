@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/creesullivan/cephean/distortion>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-cephean-distortion.html> ;
        modgui:stylesheet <modgui/stylesheet-cephean-distortion.css> ;
        modgui:screenshot <modgui/screenshot-cephean-distortion.png> ;
        modgui:thumbnail <modgui/thumbnail-cephean-distortion.png> ;
        modgui:brand "cephean" ;
        modgui:label "Distortion" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "darkblue" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "drive" ;
            lv2:name "Drive" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "shape" ;
            lv2:name "Shape" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "dirty" ;
            lv2:name "Dirty" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "detail" ;
            lv2:name "Detail" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "color" ;
            lv2:name "Color" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "level" ;
            lv2:name "Level" ;
        ] ;
    ] .